<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="415.0" prefWidth="595.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AtrakcjeController">
   <children>
      <TableView fx:id="TableAtrakcje" layoutX="14.0" layoutY="72.0" prefHeight="219.0" prefWidth="478.0">
        <columns>
          <TableColumn fx:id="TableAtrakcjeID" prefWidth="44.0" text="ID" />
          <TableColumn fx:id="TableAtrakcjeDzien" prefWidth="58.0" text="Dzien" />
            <TableColumn fx:id="TableAtrakcjeOd" prefWidth="51.0" text="Od" />
            <TableColumn fx:id="TableAtrakcjeDo" prefWidth="57.0" text="Do" />
            <TableColumn fx:id="TableAtrakcjeNazwa" prefWidth="70.0" text="Nazwa" />
            <TableColumn fx:id="TableAtrakcjeOpis" prefWidth="140.0" text="Opis" />
            <TableColumn fx:id="TableAtrakcjeStrefa" prefWidth="57.0" text="Strefa" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="26.0" text="Atrakcje" textFill="#1a89f8">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="303.0" prefHeight="18.0" prefWidth="79.0" text="Dodaj atrakcje:" />
      <TextField fx:id="TextAtrakcjaID" layoutX="14.0" layoutY="329.0" promptText="ID Atrakcji" />
      <TextField fx:id="TextAtrakcjaDate" layoutX="14.0" layoutY="364.0" promptText="Dzien YYYY-MM-DD " />
      <TextField fx:id="TextAtrakcjaOd" layoutX="183.0" layoutY="300.0" prefHeight="25.0" prefWidth="59.0" promptText="Od" />
      <TextField fx:id="TextAtrakcjaDo" layoutX="265.0" layoutY="300.0" prefHeight="25.0" prefWidth="59.0" promptText="Do" />
      <TextField fx:id="TextAtrakcjaOpis" layoutX="343.0" layoutY="300.0" prefHeight="108.0" prefWidth="141.0" promptText="Opis" />
      <TextField fx:id="TextAtrakcjaNazwa" layoutX="183.0" layoutY="329.0" prefHeight="25.0" prefWidth="141.0" promptText="Nazwa" />
      <TextField fx:id="TextAtrakcjaStrefa" layoutX="183.0" layoutY="364.0" prefHeight="25.0" prefWidth="141.0" promptText="Strefa" />
      <Button fx:id="butAddAtr" layoutX="510.0" layoutY="309.0" mnemonicParsing="false" onAction="#AddAtrakcja" text="Dodaj" />
