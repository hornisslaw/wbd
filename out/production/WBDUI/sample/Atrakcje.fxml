      <Button fx:id="ButToMenu" layoutX="509.0" layoutY="364.0" mnemonicParsing="false" onAction="#ReturnToMenu" text="Powrot" />
      <TextField fx:id="SearchFieldData" layoutX="191.0" layoutY="34.0" prefHeight="25.0" prefWidth="174.0" promptText="Nazwa Atrakcji" />
      <Button fx:id="butSzukaj" layoutX="405.0" layoutY="34.0" mnemonicParsing="false" onAction="#FindEvent" text="Szukaj" />
   </children>
</AnchorPane>
